{
  "name": "@bunny.net/cli",
  "version": "0.1.0",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "bin": {
    "bunny": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "ny": "bun run src/index.ts",
    "build": "bun build src/index.ts --compile --outfile bunny",
    "typecheck": "bun run tsc --noEmit",
    "api:generate": "openapi-typescript",
    "api:update": "bun run scripts/update-specs.ts && bun run api:generate"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.35",
    "openapi-typescript": "^7.13.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@libsql/client": "^0.17.0",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "openapi-fetch": "^0.17.0",
    "ora": "^9.3.0",
    "prompts": "^2.4.2",
    "yargs": "^18.0.0",
    "zod": "^4.3.6"
  }
}
